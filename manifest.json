{
    "manifest_version": 2,
    "name": "Busca FQA",
    "version": "1.0",
    "description": "Barra de busca em FQA",
    "browser_action": {
        "default_popup": "index.html"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["CSS/style.css"],
            "js": ["script.js"]
        }
    ]


}